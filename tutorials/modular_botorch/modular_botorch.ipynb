            "[W 250502 15:19:17 cholesky:40] A not p.d., added jitter of 1.0e-08 to the diagonal\n",
            "[W 250502 15:19:22 cholesky:40] A not p.d., added jitter of 1.0e-08 to the diagonal\n",
            "[W 250502 15:19:26 cholesky:40] A not p.d., added jitter of 1.0e-08 to the diagonal\n",
            "[W 250502 15:19:28 cholesky:40] A not p.d., added jitter of 1.0e-08 to the diagonal\n"
          ]
        }
      ],
      "source": [
        "for _ in range(10):\n",
        "    trials = client.get_next_trials(max_trials=1)\n",
        "    for index, parameters in trials.items():\n",
        "        result = test_function(**parameters)\n",
        "        client.complete_trial(trial_index=index, raw_data={\"test_metric\": result})"
      ]
